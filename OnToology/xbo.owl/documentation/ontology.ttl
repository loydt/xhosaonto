#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/XBO#hasAgeGroup
:hasAgeGroup rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "Relates a Person or Item and the required  Age  group associated with the use of Item. hasAgeGroup specifies the Age interval or range for which the Item or Person is valid. This allows specifying that a certain  Item is valid only for a certain Age group." ;
             rdfs:label "hasAgeGroup" .


###  http://www.semanticweb.org/XBO#hasBodyType
:hasBodyType rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "Relates Item to the type of body that the Item is equipped with as determined in the entity ItemBody." ;
             rdfs:label "hasBodyType" .


###  http://www.semanticweb.org/XBO#hasChild
:hasChild rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:comment "Relates a Person to another Person (object) who is a child of that Person." ;
          rdfs:label "hasChild" .


###  http://www.semanticweb.org/XBO#hasColour
:hasColour rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:comment "Relates an Item to the presence of Colour in the makeup or content of an Item." ;
           rdfs:label "hasColour" .


###  http://www.semanticweb.org/XBO#hasDecoration
:hasDecoration rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:comment "Relates a specific elaborate design feature or theme of Decoration to an Item." ;
               rdfs:label "hasDecoration" .


###  http://www.semanticweb.org/XBO#hasFather
:hasFather rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:comment "Associates a Person to another Person who is the biological Father of that Person." ;
           rdfs:label "hasFather" .


###  http://www.semanticweb.org/XBO#hasMaterial
:hasMaterial rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "Associates an Item to a type of underlying physical Material used to make or is incorporated in the Item." ;
             rdfs:label "hasMaterial" .


###  http://www.semanticweb.org/XBO#hasMother
:hasMother rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:comment "Relates a Person to another Person who is the biological Mother of that Person." ;
           rdfs:label "hasMother" .


###  http://www.semanticweb.org/XBO#hasParent
:hasParent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:comment "Associates  a Person (object)  to another to another Person who is a parent of that Person." ;
           rdfs:label "hasParent" .


###  http://www.semanticweb.org/XBO#hasPosition
:hasPosition rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "Associates the Item with the recommended Position that denotes a point within some BodyRegion where the Item is intended to occupy or be placed or worn." ;
             rdfs:label "hasPosition" .


###  http://www.semanticweb.org/XBO#hasQuantity
:hasQuantity rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:comment "Associates an Item with a unit of measure Quantity, where Quantity is how much or proportion there is of an Item, Colour or Material. For example, this allows specifying that a certain Quantity is valid only for a certain Item." ;
             rdfs:label "hasQuantity" .


###  http://www.semanticweb.org/XBO#hasRank
:hasRank rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:comment "Relates a Person or Item to the appropriate type of level or rank or status held by a Person or Item according to classifications or social structure defined in Rank." ;
         rdfs:label "hasRank" .


###  http://www.semanticweb.org/XBO#hasSex
:hasSex rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain :DomainEntity ;
        rdfs:range :Sex ;
        rdfs:comment "Links a Person or Item to the values Male or Female  describing Sex. Person or Item is related to an instance of Sex." ;
        rdfs:label "hasSex" .


###  http://www.semanticweb.org/XBO#hasSide
:hasSide rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:comment "Relates an Item to the Side on which to put the Item relative to a centre or reference location such as the trunk of the human body." ;
         rdfs:label "hasSide" .


###  http://www.semanticweb.org/XBO#hasSize
:hasSize rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         rdfs:comment "Associates the physical magnitude or dimensions used to represent an Item to Size." ;
         rdfs:label "hasSize" .


###  http://www.semanticweb.org/XBO#hasSpouse
:hasSpouse rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:comment "Associates a Person to another Person in a shared activity of common interest such as a partnership of traditional marriage or common law marriage or  civil union." ;
           rdfs:label "hasSpouse" .


###  http://www.semanticweb.org/XBO#hasTribe
:hasTribe rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:comment "Associates an Item or Person to a Tribe meaning a group or social division of people an Item or Person belongs to. Tribe is a characteristic such as Thembu, Pondo or Bomvana." ;
          rdfs:label "hasTribe" .


###  http://www.semanticweb.org/XBO#hasUse
:hasUse rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf owl:topObjectProperty ;
        rdfs:comment "Relates an Item or Person to the appropriate or recommended category of Use for which the Item is intended." ;
        rdfs:label "hasUse" .


###  http://www.semanticweb.org/XBO#isWornBy
:isWornBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:comment "Links the Item to the recommended  Person for whom the Item is considered suitable or  eligible  Age to adorn or wear  or put on the   Item." ;
          rdfs:label "isWornBy" .


###  http://www.semanticweb.org/XBO#isWornOn
:isWornOn rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:comment "Associates an Item with a particular  place, location or region of the human body occupied by an Item as represented in BodyRegion." ;
          rdfs:label "isWornOn" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/XBO#ageGroup
:ageGroup rdf:type owl:DatatypeProperty ;
          rdfs:domain :Age ;
          rdfs:range xsd:double ;
