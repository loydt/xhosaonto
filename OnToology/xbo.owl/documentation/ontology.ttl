              rdfs:range xsd:integer ;
              rdfs:label "itemQuantity" ;
              <http://www.w3.org/2004/02/skos/core#prefLabel> "item quantity" .


###  http://www.semanticweb.org/XBO#itemSize
:itemSize rdf:type owl:DatatypeProperty ;
          rdfs:domain :Size ;
          rdfs:range xsd:double ;
          rdfs:label "itemSize" ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "item size" .


###  http://www.semanticweb.org/XBO#itemUse
:itemUse rdf:type owl:DatatypeProperty ;
         rdfs:domain :Use ;
         rdfs:range xsd:string ;
         rdfs:label "itemUse" ;
         <http://www.w3.org/2004/02/skos/core#prefLabel> "item use" .


###  http://www.semanticweb.org/XBO#localName
:localName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Item ;
           rdfs:range xsd:string ;
           rdfs:label "localName" ;
           <http://www.w3.org/2004/02/skos/core#prefLabel> "local name" .


###  http://www.semanticweb.org/XBO#materialType
:materialType rdf:type owl:DatatypeProperty ;
              rdfs:domain :Material ;
              rdfs:range xsd:string ;
              rdfs:label "materialType" ;
              <http://www.w3.org/2004/02/skos/core#prefLabel> "material Type" .


###  http://www.semanticweb.org/XBO#personType
:personType rdf:type owl:DatatypeProperty ;
            rdfs:domain :Person ;
            rdfs:range xsd:string ;
