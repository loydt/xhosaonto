###  http://www.semanticweb.org/XBO#socialRank
:socialRank rdf:type owl:DatatypeProperty ;
            rdfs:domain :Rank ;
            rdfs:range xsd:string ;
            rdfs:label "socialRank" ;
            <http://www.w3.org/2004/02/skos/core#prefLabel> "social rank" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/XBO#Adolescence
:Adolescence rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Age
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :ageGroup ;
                                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                               owl:onDatatype xsd:double ;
                                                                               owl:withRestrictions ( [ xsd:minExclusive "13.0"^^xsd:double
                                                                                                      ]
                                                                                                      [ xsd:maxExclusive "18.0"^^xsd:double
                                                                                                      ]
                                                                                                    )
                                                                             ]
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Age ;
             rdfs:label "Adolescence"@en ;
             <http://www.w3.org/2004/02/skos/core#prefLabel> "Adolescence"@en .


###  http://www.semanticweb.org/XBO#Adulthood
:Adulthood rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Age
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :ageGroup ;
                                                        owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                             owl:onDatatype xsd:double ;
                                                                             owl:withRestrictions ( [ xsd:minExclusive "19.0"^^xsd:double
                                                                                                    ]
                                                                                                    [ xsd:maxExclusive "120.0"^^xsd:double
                                                                                                    ]
                                                                                                  )
                                                                           ]
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Age ;
           rdfs:label "Adulthood"@en ;
           <http://www.w3.org/2004/02/skos/core#prefLabel> "Adulthood"@en .


###  http://www.semanticweb.org/XBO#Age
:Age rdf:type owl:Class ;
     owl:equivalentClass [ rdf:type owl:Class ;
                           owl:unionOf ( :Adolescence
                                         :Adulthood
                                         :Childhood
                                         :Infanthood
                                       )
                         ] ;
     rdfs:subClassOf :ValuePartition ;
     rdfs:comment "Age  is a  ValuePartition that describes only values from  subclasses Infanthood, Adolescence, Childhood or Adulthood . NB These subclasses can themselves be further partitioned." ;
     rdfs:label "Age"@en ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Age"@en .


###  http://www.semanticweb.org/XBO#Ankle
:Ankle rdf:type owl:Class ;
       rdfs:subClassOf :BodyRegion ;
       rdfs:label "Ankle"@en ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Ankle"@en .


###  http://www.semanticweb.org/XBO#Anklet
:Anklet rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Item
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :isWornOn ;
                                                     owl:someValuesFrom :Ankle
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Item ;
        rdfs:comment "The class  Anklet is used for concepts that represent a beadwork Item in form of a  string or band or ring worn around the Ankle." ;
        rdfs:label "Anklet"@en ;
        <http://www.w3.org/2004/02/skos/core#prefLabel> "Anklet"@en .


###  http://www.semanticweb.org/XBO#Arm
:Arm rdf:type owl:Class ;
     rdfs:subClassOf :BodyRegion ;
     rdfs:label "Arm"@en ;
     <http://www.w3.org/2004/02/skos/core#prefLabel> "Arm"@en .


###  http://www.semanticweb.org/XBO#Armlet
:Armlet rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Item
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :isWornOn ;
                                                     owl:someValuesFrom :Arm
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Item ;
        rdfs:comment "The class  Armlet is used to represent a beadwork Item  in form of a string or band or ring worn around the Upper Arm or  high on the Arm" ;
        rdfs:label "Armlet"@en ;
        <http://www.w3.org/2004/02/skos/core#prefLabel> "Armlet"@en .


###  http://www.semanticweb.org/XBO#BeadworkEntity
:BeadworkEntity rdf:type owl:Class ;
                rdfs:subClassOf :DomainEntity ;
                owl:disjointWith :PersonEntity ;
                rdfs:comment "Class  BeadworkEntity comprise concepts that represent objects or items of beadwork and the associated description of the item with respect to the accepted statues of Xhosa Beadwork." ;
                rdfs:label "BeadworkEntity"@en ;
                <http://www.w3.org/2004/02/skos/core#prefLabel> "Beadwork Entity"@en .


###  http://www.semanticweb.org/XBO#Bhaca
:Bhaca rdf:type owl:Class ;
       rdfs:subClassOf :Tribe ;
       rdfs:label "Bhaca"@en ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Bhaca"@en .


###  http://www.semanticweb.org/XBO#BodyRegion
:BodyRegion rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:unionOf ( :Ankle
                                                :Arm
                                                :Head
                                                :Neck
                                                :Shin
                                                :Waist
                                                :Wrist
                                              )
                                ] ;
            rdfs:subClassOf :ValuePartition ;
            rdfs:comment "BodyRegion is a  ValuePartition that describes only values from  subclasses Neck, Waist, Arm, Shin, Head, Ankle or Wrist. NB These subclasses can themselves be further partitioned." ;
            rdfs:label "BodyRegion"@en ;
            <http://www.w3.org/2004/02/skos/core#prefLabel> "Body Region"@en .


###  http://www.semanticweb.org/XBO#BodyType
:BodyType rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( :Flexible
                                              :Inflexible
                                            )
                              ] ;
          rdfs:subClassOf :ValuePartition ;
          rdfs:comment "BodyType is a  ValuePartition that describes only values from  subclasses  Flexible  or Inflexible. NB These subclasses can themselves be further partitioned." ;
          rdfs:label "BodyType"@en ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "Body Type"@en .


###  http://www.semanticweb.org/XBO#Bomvana
:Bomvana rdf:type owl:Class ;
         rdfs:subClassOf :Tribe ;
         rdfs:label "Bomvana"@en ;
         <http://www.w3.org/2004/02/skos/core#prefLabel> "Bomvana"@en .


###  http://www.semanticweb.org/XBO#Bracelet
:Bracelet rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Item
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :isWornOn ;
                                                       owl:someValuesFrom :Wrist
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Item ;
          rdfs:comment "The class Bracelet represent an article or beadwork Item in form of a loop or band encircling or worn around the Wrist." ;
          rdfs:label "Bracelet"@en ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "Bracelet"@en .


###  http://www.semanticweb.org/XBO#Ceremony
:Ceremony rdf:type owl:Class ;
          rdfs:subClassOf :Use ;
          owl:disjointWith :Ritual ;
          rdfs:label "Ceremony"@en ;
          <http://www.w3.org/2004/02/skos/core#prefLabel> "Ceremony"@en .


###  http://www.semanticweb.org/XBO#Charm
:Charm rdf:type owl:Class ;
       rdfs:subClassOf :Decoration ;
       owl:disjointWith :Fringe ,
                        :Strand ,
                        :Streamer ,
                        :Tab ,
                        :Tassel ;
       rdfs:label "Charm"@en ;
       <http://www.w3.org/2004/02/skos/core#prefLabel> "Charm"@en .


###  http://www.semanticweb.org/XBO#CharmNecklace
:CharmNecklace rdf:type owl:Class ;
               owl:equivalentClass [ owl:intersectionOf ( :Necklace
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :hasDecoration ;
                                                            owl:someValuesFrom :Charm
                                                          ]
                                                        ) ;
                                     rdf:type owl:Class
                                   ] ;
               rdfs:subClassOf :Necklace ;
               rdfs:label "CharmNecklace"@en ;
               <http://www.w3.org/2004/02/skos/core#prefLabel> "CharmNecklace"@en .


###  http://www.semanticweb.org/XBO#Childhood
:Childhood rdf:type owl:Class ;
           owl:equivalentClass [ owl:intersectionOf ( :Age
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :ageGroup ;
                                                        owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                             owl:onDatatype xsd:double ;
                                                                             owl:withRestrictions ( [ xsd:minExclusive "2.0"^^xsd:double
                                                                                                    ]
                                                                                                    [ xsd:maxExclusive "12.0"^^xsd:double
                                                                                                    ]
                                                                                                  )
                                                                           ]
                                                      ]
                                                    ) ;
                                 rdf:type owl:Class
                               ] ;
           rdfs:subClassOf :Age ;
           rdfs:label "Childhood"@en ;
           <http://www.w3.org/2004/02/skos/core#prefLabel> "Childhood"@en .


###  http://www.semanticweb.org/XBO#ChokerNecklace
:ChokerNecklace rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Necklace
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :hasPosition ;
                                                             owl:someValuesFrom :Upper
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Necklace ;
                rdfs:label "ChokerNecklace"@en ;
                <http://www.w3.org/2004/02/skos/core#prefLabel> "ChokerNecklace"@en .


###  http://www.semanticweb.org/XBO#CollarNecklace
:CollarNecklace rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Necklace
